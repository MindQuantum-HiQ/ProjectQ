  set(MINGW 1)
endif()
set(CMAKE_DPCXX_COMPILER_ID_RUN 1)
set(CMAKE_DPCXX_SOURCE_FILE_EXTENSIONS C;M;c++;cc;cpp;cxx;m;mm;mpp;CPP;ixx;cppm;dpcpp)
set(CMAKE_DPCXX_IGNORE_EXTENSIONS inl;h;hpp;HPP;H;o;O;obj;OBJ;def;DEF;rc;RC)

foreach(lang C CXX OBJC OBJCXX)
  if(CMAKE_${lang}_COMPILER_ID_RUN)
    foreach(extension IN LISTS CMAKE_${lang}_SOURCE_FILE_EXTENSIONS)
      list(REMOVE_ITEM CMAKE_DPCXX_SOURCE_FILE_EXTENSIONS ${extension})
    endforeach()
  endif()
endforeach()

set(CMAKE_DPCXX_LINKER_PREFERENCE 25) # Lower than CXX to avoid "stealing" files from C++
set(CMAKE_DPCXX_LINKER_PREFERENCE_PROPAGATES 1)

